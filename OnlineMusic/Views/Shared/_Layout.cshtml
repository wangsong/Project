<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta rel=""/>
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <link href="../../Content/onlinemusic-imageSlide.css" rel="stylesheet" type="text/css" />
    <script src="../../Scripts/wangsong-returntop.js" type="text/javascript"></script>
    <link href="../../Content/onlinemusic-returntop.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/onlinemusic-onlinestart.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/onlinemusic-rank.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div id="main_frame">
        <div id="header">
            <div id="logo">
                <a href="../Home/Index">
                    <img src="../../Content/themes/images/logo2.png" alt="" title="Go Home" />
                </a>
                <a class="login" href="/Account/">Log In</a>
                <a class="login" href="@Url.Action("Index","OnlineMusicManager")">Manager</a>
            </div>
        </div>
        <hr color="#00BB00" size="1px"/>
        <!--menu-->
        @Html.Action("Menu", "Common")
        <div id="Container">
            <!--main-frame-->
            @RenderBody()
        </div>
        @{Html.RenderAction("Footer", "Common");}
    </div>
    <div id="goTopBtn" class="goTopBtn">
        <a href="javascript:void(0)" title="Return Top"></a>
    </div>
</body>
</html>
